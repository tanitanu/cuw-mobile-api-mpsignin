{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "United.Ebs.Logging.Providers.LogFormatter, United.Ebs.Logging"
        }
      }
    ],
    "Properties": {
      "Application": "United.Mobile.MPAuthentication.Api"
    }
  },
  "EnableFeatureSettingsChanges": true
}