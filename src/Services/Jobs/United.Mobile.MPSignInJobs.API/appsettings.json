{
  "Logging": {
    "IncludeScopes": false,
    "CSLLogging": {
      "RequestResponseEnabled": "true",
      "DateFormat": "MM/dd/yyyy HH:mm:ss.ffffffzzz",
      "InnerExceptionLength": 5000,
      "StackTraceLength": 5000
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Override": {
        "Default": "Information",
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "United.Ebs.Logging.Providers.LogFormatter, United.Ebs.Logging"
        }
      }
    ],
    "Properties": {
      "Application": "United.Mobile.MPSignInJobs.API"
    }
  },
  "OnPremSQLDataMigrationService": {
    "baseUrl": "https://smartphone-dev.united.com/OnPremSQL2DynamoService/api",
    "timeoutPolicyConfig": {
      "seconds": 180
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "CloudSQLDataMigrationService": {
    "baseUrl": "https://mobileapi.dev.cuw.aws.ual.com/sqltodynamoservice/api/",
    "timeoutPolicyConfig": {
      "seconds": 180
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "cachingConfig": {
    "baseUrl": "https://mobileapi.dev.cuw.aws.ual.com/cacheservice/api/",
    "timeoutPolicyConfig": {
      "seconds": 180
    },
    "retryPolicyConfig": {
      "RetryCount": 1
    },
    "circuitBreakerPolicyConfig": {
      "AllowExceptions": 1,
      "BreakDuration": 30
    }
  },
  "TriggerDevice_2DynamoDBJobInSeconds": 5280,
  "TriggerMileagePlusValidation_CSS2DynamoDBJobInSeconds": 5280,
  "EnableMPSignInJobs": false
}
